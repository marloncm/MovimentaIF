<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    
    <!-- Haltere simplificado e mais visÃ­vel -->
    <group
        android:translateX="54"
        android:translateY="54">
        
        <!-- Disco esquerdo externo -->
        <path
            android:fillColor="#FFFFFF"
            android:pathData="M-35,-18 L-20,-18 L-20,18 L-35,18 Z" />
        
        <!-- Disco esquerdo interno -->
        <path
            android:fillColor="#E0E0E0"
            android:pathData="M-32,-15 L-23,-15 L-23,15 L-32,15 Z" />
        
        <!-- Barra esquerda -->
        <path
            android:fillColor="#757575"
            android:pathData="M-20,-10 L-8,-10 L-8,10 L-20,10 Z" />
        
        <!-- Centro (pegada) -->
        <path
            android:fillColor="#424242"
            android:pathData="M-8,-12 L8,-12 L8,12 L-8,12 Z" />
        
        <!-- Linhas de textura no grip -->
        <path
            android:strokeColor="#212121"
            android:strokeWidth="1.5"
            android:pathData="M-6,-10 L-6,10 M-3,-10 L-3,10 M0,-10 L0,10 M3,-10 L3,10 M6,-10 L6,10" />
        
        <!-- Barra direita -->
        <path
            android:fillColor="#757575"
            android:pathData="M8,-10 L20,-10 L20,10 L8,10 Z" />
        
        <!-- Disco direito interno -->
        <path
            android:fillColor="#E0E0E0"
            android:pathData="M23,-15 L32,-15 L32,15 L23,15 Z" />
        
        <!-- Disco direito externo -->
        <path
            android:fillColor="#FFFFFF"
            android:pathData="M20,-18 L35,-18 L35,18 L20,18 Z" />
        
        <!-- Sombra inferior para profundidade -->
        <path
            android:fillColor="#000000"
            android:fillAlpha="0.3"
            android:pathData="M-35,15 L35,15 L35,18 L-35,18 Z" />
        
        <!-- Destaque superior -->
        <path
            android:fillColor="#FFFFFF"
            android:fillAlpha="0.4"
            android:pathData="M-35,-18 L35,-18 L35,-15 L-35,-15 Z" />
        
    </group>
    
</vector>